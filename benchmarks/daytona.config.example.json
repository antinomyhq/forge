{
  "daytona": {
    "apiKey": "${DAYTONA_API_KEY}",
    "workspace": {
      "language": "rust",
      "image": null,
      "resourceAllocation": {
        "cpu": "2",
        "memory": "4GB",
        "storage": "10GB"
      }
    },
    "execution": {
      "maxConcurrency": 10,
      "maxRetries": 3,
      "retryDelayMs": 2000,
      "timeout": 300
    },
    "build": {
      "binaryPath": "/workspace/target/release/forge",
      "buildCommand": "cargo build --release",
      "cacheBinaries": true
    }
  },
  "logging": {
    "level": "info",
    "json": false
  }
}
